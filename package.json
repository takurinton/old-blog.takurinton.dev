    "node-fetch": "^2.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.2.1",
    "rss-parser": "^3.12.0",
    "styled-components": "^5.3.3",
    "use-sync-external-store": "^1.0.0-rc.0"
  }
}
