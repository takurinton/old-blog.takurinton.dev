  "devDependencies": {
    "@takurinton/eslint-plugin-limit-import-scope": "0.0.6",
    "@takurinton/tsserver-plugin-limit-import-scope": "latest",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/marked": "^4.0.1",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.12.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn",
    "urql/debug": "cd shared/@takurinton/urql && yarn fix && yarn build && cd - && yarn reinstall && yarn dev",
    "dev": "yarn dev/client & yarn dev/server",
    "build": "rm -rf ./dist && yarn build/server && yarn build/client",
    "dev/server": "ts-node-dev ./server/index.ts",
    "dev/client": "esbuild --bundle ./client/main.tsx --outfile=./dist/main.js --watch",
    "build/server": "esbuild --bundle ./server/index.ts --outfile=./dist/server.js --platform=node --minify --tree-shaking=true",
    "build/client": "esbuild --minify --bundle ./client/main.tsx --outfile=./dist/main.js --tree-shaking=true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "vercel": "yarn build/client && yarn build/server && vercel --prod",
    "lint": "yarn lint:client && yarn lint:server",
    "lint:client": "eslint './client/**/*.{ts,tsx}'",
    "lint:server": "eslint './server/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takurinton/blog.takurinton.dev.git"
  },
